  "version": "0.2.0",
  "configurations": [
    {
      "name": "Preview in Copilot (Edge)",
      "type": "msedge",
      "request": "launch",
      "url": "https://www.office.com/chat?auth=2",
      "presentation": {
          "group": "remote",
          "order": 1
      },
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Preview in Copilot (Chrome)",
      "type": "chrome",
      "request": "launch",
      "url": "https://www.office.com/chat?auth=2",
      "presentation": {
          "group": "remote",
          "order": 2
      },
      "internalConsoleOptions": "neverOpen"
    },
    {
      "name": "Excel Desktop (Edge Chromium)",
      "type": "msedge",
      "request": "attach",
      "port": 9229,
      "timeout": 600000,
      "webRoot": "${workspaceRoot}",
      "preLaunchTask": "Debug: Excel Desktop",
      "postDebugTask": "Stop Debug"
    },
    {
      "name": "Excel Desktop (Edge Legacy)",
      "type": "office-addin",
      "request": "attach",
      "url": "https://localhost:3000/taskpane.html?_host_Info=Excel$Win32$16.01$en-US$$$$0",
      "port": 9222,
      "timeout": 600000,
      "webRoot": "${workspaceRoot}",
      "preLaunchTask": "Debug: Excel Desktop",
      "postDebugTask": "Stop Debug"
    },
